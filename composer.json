{
    "name": "elife/logging-sdk",
    "description": "eLife Sciences logging SDK",
    "type": "library",
    "license": "MIT",
    "require": {
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "suggest": {
        "ext/newrelic": "Enables the Monitoring class"
    },
    "autoload": {
        "psr-4": {
            "eLife\\Logging\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\Logging\\": "./tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
